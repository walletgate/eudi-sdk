# WalletGate EUDI SDK Configuration
# Copy this file to .env for development

# API Configuration
WALLETGATE_API_KEY=your_api_key_here
WALLETGATE_BASE_URL=https://api.walletgate.app

# For local development against self-hosted instance
# WALLETGATE_BASE_URL=http://localhost:4000

# Test Configuration
# These are used for running integration tests
WALLETGATE_TEST_API_KEY=test_api_key_here
WALLETGATE_TEST_BASE_URL=http://localhost:4000

# Webhook Configuration
WEBHOOK_SECRET=your_webhook_secret_here

# Environment
NODE_ENV=development

# Optional: Enable debug logging
DEBUG=walletgate:*